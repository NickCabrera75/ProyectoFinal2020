<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/ivLogo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="109dp"
        android:layout_marginTop="269dp"
        android:layout_marginEnd="110dp"
        android:layout_marginBottom="270dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo_size2" />

    <TextView
        android:id="@+id/tvContacto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="14dp"
        android:layout_marginEnd="97dp"
        android:layout_marginBottom="30dp"
        android:fontFamily="@font/audiowide"
        android:text="safesoft.sa@gmail.com"
        android:textColor="#2196F3"
        app:layout_constraintBottom_toTopOf="@+id/tvIntegrantes"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/ivContacto" />

    <TextView
        android:id="@+id/tvIntegrantes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="13dp"
        android:layout_marginEnd="108dp"
        android:layout_marginBottom="222dp"
        android:fontFamily="@font/audiowide"
        android:text="Cabrera Vargas Nick"
        android:textColor="#2196F3"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/ivIntegrantes" />

    <ImageView
        android:id="@+id/ivContacto"
        android:layout_width="20dp"
        android:layout_height="19dp"
        android:layout_marginStart="110dp"
        android:layout_marginEnd="14dp"
        android:layout_marginBottom="28dp"
        android:background="#2196F3"
        app:layout_constraintBottom_toTopOf="@+id/ivIntegrantes"
        app:layout_constraintEnd_toStartOf="@+id/tvContacto"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@android:drawable/ic_dialog_email" />

    <ImageView
        android:id="@+id/ivIntegrantes"
        android:layout_width="20dp"
        android:layout_height="19dp"
        android:layout_marginStart="110dp"
        android:layout_marginEnd="13dp"
        android:layout_marginBottom="222dp"
        android:background="#2196F3"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/tvIntegrantes"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@android:drawable/ic_menu_myplaces" />
</androidx.constraintlayout.widget.ConstraintLayout>